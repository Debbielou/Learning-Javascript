function myFunction() {
  let a = 4;
  return a * a;
}
console.log(a); // ReferenceError: a is not defined

function test(){
  b = 5;
}
console.log(b); // ReferenceError: b is not defined
console.log(test()); // undefined

let a = 4;
function myFunction() {
  return a * a;
}
console.log(myFunction()); // 16